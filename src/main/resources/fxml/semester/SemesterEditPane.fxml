<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<fx:root prefHeight="400.0" prefWidth="400.0" stylesheets="@../../css/styles.css" type="AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <AnchorPane prefHeight="87.0" prefWidth="400.0" styleClass="theme-main" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
              <Text fx:id="txt_title" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="edit-header" text="Semester" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="0.0" />
         </children>
      </AnchorPane>
      <VBox alignment="CENTER" fillWidth="false" prefWidth="400.0" spacing="10.0" style="-fx-background-color: #fff;" stylesheets="@../../css/styles.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="87.0">
          <children>
              <Text fx:id="txt_error" fill="#fc0808" strokeType="OUTSIDE" strokeWidth="0.0" text="Please enter a valid year." visible="false">
                  <font>
                      <Font size="15.0" />
                  </font>
              </Text>
            <ChoiceBox fx:id="cb_name" prefWidth="200.0" styleClass="edit-text" />
            <TextField fx:id="tf_year" prefWidth="200.0" promptText="year" styleClass="edit-text">
               <VBox.margin>
                  <Insets bottom="30.0" />
               </VBox.margin></TextField>
            <Button fx:id="btn_save" mnemonicParsing="false" prefWidth="200.0" styleClass="button_primary" text="Save" />
            <Button fx:id="btn_cancel" mnemonicParsing="false" prefWidth="200.0" styleClass="button_alt" text="Cancel" />
          </children>
      </VBox>
   </children>
</fx:root>
